<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
   <servlet>
  	<servlet-class>com.jsp.student_crud.controller.StudentInsertController</servlet-class>
  	<servlet-name>register</servlet-name>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>register</servlet-name>
   <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-class>com.jsp.student_crud.controller.StudentDeleteController</servlet-class>
  <servlet-name>delete</servlet-name>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/delete</url-pattern>
  </servlet-mapping>
  
   <servlet>
  <servlet-class>com.jsp.student_crud.controller.StudentUpdateController</servlet-class>
  <servlet-name>update</servlet-name>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>update</servlet-name>
  <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
    <servlet>
  <servlet-class>com.jsp.student_crud.controller.StudentDispalyController</servlet-class>
  <servlet-name>display</servlet-name>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>display</servlet-name>
  <url-pattern>/display</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-class>com.jsp.student_crud.controller.LoginController</servlet-class>
  	<servlet-name>myLogin</servlet-name>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>myLogin</servlet-name>
   <url-pattern>/myLogin</url-pattern>
  </servlet-mapping>
  
</web-app>
